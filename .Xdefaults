! Xft
!Xft.antialias: 1
!Xft.autohint: 1
!Xft.dpi: 96
!Xft.dpi: 72
!Xft.hinting: true Xft.hintstyle: hintfull Xft.rgba: rgb
!Xft.lcdfilter: lcddefault

Gdraw.ScreenWidthInches: 23
Gdraw.ScreenWidthCentimeters: 70

! URxvt ----------------
URxvt*depth: 24
URxvt.perl-ext:
!URxvt.perl-ext-common:    default,vtwheel
URxvt.perl-ext-common:    default,tabbedex,vtwheel
URxvt.tabbed.tabbar-fg:   136
!URxvt.tabbed.tabbar-bg:   #c2c2c2
URxvt.tabbed.tab-fg:      3
URxvt.tabbed.tab-bg:      0
URxvt.tabbed.autohide:    yes
URxvt.tabbed.new-button:  no
URxvt.tabbed.title:       no
urxvt*scrollBar:          false
!urxvt*scrollBar_right:   true
!urxvt*scrollColor:        #888888
!urxvt*throughColor:        #000000
!urxvt*borderColor:        #888888
!urxvt*scrollstyle:        xterm
!urxvt*scrollstyle:       rxvt / plain, next, xterm
!urxvt*thickness:       number
!urxvt*scrollBar:          true

! Fonts -------------------
! Thu 19.06 2014
!*font: xft:Cousine for Powerline:size=10
! 18.06.2015
!*font: xft:PragmataPro\ for\ Powerline:size=11
! Last 2016.12.17
! Compile rxvt with flags: --enable-frills --enable-unicode3 (https://github.com/powerline/powerline/issues/684)
*font: xft:Pragmata\ Pro\ Mono:size=12
URxvt.letterSpace:        -1
!URxvt.lineSpace:          1
! Iosevka
! *font: xft:Iosevka:size=11
! URxvt.letterSpace:        -1

